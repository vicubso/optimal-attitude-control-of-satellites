issn = {24058963},
journal = {IFAC-PapersOnLine},
keywords = {Adaptive feedforward controller,Bang-Bang controller,Dry friction,Electromechanical clutch actuator,Hysteresis},
month = {jul},
number = {1},
pages = {4787--4793},
publisher = {Elsevier B.V.},
title = {{Control of an electromechanical clutch actuator by a parallel Adaptive Feedforward and Bang-Bang controller: Simulation and Experimental results}},
