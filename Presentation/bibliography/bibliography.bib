@book{Pedregal2010,
abstract = {This is a basic text on the mathematical theory for optimal control problems in the sense that it addresses the most basic and pertinent questions in this field of applied mathematics. The main novelty in the text is the perspective to deal with those in a special way, as we systematically pursue the strategy of taking back any given optimal control situation to an equivalent variational problem. The title makes an attempt to convey this driving idea. In this way, basic results of existence of optimal solutions, optimality, and even approximation are examined from this standpoint. There are however a bunch of important and profound topics which are not deliberately treated here, as we regard them well beyond the scope of this text. These include, but are not limited to: • controllability; • observability; • singular controls; • state constraints; • non-existence; • incomplete information; • stochastic optimal control; • techniques for optimal control in realistic situations. The book can serve for a first course in optimal control within the context of a master program in applied mathematics or engineering; and it can also be helpful to more advanced students who would like to learn what the variational method, for optimal control, is about. It can also be useful for a second part of a course in variational principles. Prerequisites include, in addition to Linear Algebra, Multivariate Calculus, and Differential Equations, basic Measure Theory (results like the Lebesque differentia- tion theorem or the Riesz representation theorem will be invoked without a proof), and Functional Analysis including basic material on Sobolev spaces (results like the Brower fixed point theorem will also be used without a proof). A number of explicit examples have been described to motivate the subject, and to provide a set of test cases to check on the numerical approximation},
address = {(accepted)},
author = {Pedregal, Pablo},
file = {:Users/vicubso/Dropbox/Mendeley/File{\_}organizer/Pedregal/Pedregal{\_}A variational approach to optimal control for ODEs{\_}Unknown.pdf:pdf},
institution = {Universidad de Castilla - la Mancha},
publisher = {Graduate Studies in Mathematics, AMS},
title = {{A variational approach to optimal control for ODEs}}
}
